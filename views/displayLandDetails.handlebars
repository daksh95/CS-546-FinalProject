<nav class="navbar navbar-default">
    <div class="container-fluid">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">WebSite Name</a>
        </div>
        <ul class="nav navbar-nav">
            <li><a href="/land">Home</a></li>
            <li><a href="#">My Property 1</a></li>
            <li><a href="#">My Transactions</a></li>
            <li><a href="#">Profile</a></li>
        </ul>
    </div>
</nav>


<main>
    <div class="row">
        <div class="col-md-8">
            <h2>Name: {{land.address}}</h2>
            <h2>Dimensions: {{land.dimensions.length}}x{{land.dimensions.breadth}}</h2>
            <h2>Type: {{land.type}}</h2>
            <h2>Restrictions: </h2>
            <ul>
                {{#each land.restrictions}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <h2>Sale: {{land.sale.onSale}}</h2>
            {{#if land.sale.onSale}}
            <h2>Price: {{land.sale.price}}</h2>
            <h2>Date of Listing: {{land.sale.dateOfListing}}</h2>
            {{/if}}
        </div>
        <div class="col-md-4">
            {{#if isOwner}}
            <a href="#"><button>Edit Property</button></a>
            <a href="#"><button>View Transaction</button></a>
            {{else}}
            <h2>Contact Info: {{owner.name}} ({{owner.phone}}) - {{owner.emailId}}</h2>
            {{#if land.sale.onSale}}
            {{!-- Bid Form --}}
            {{/if}}
            {{/if}}
        </div>
    </div>
</main>